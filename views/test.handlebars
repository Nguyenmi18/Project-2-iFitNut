    <div id="previousStats">
    <h3>Last 5 Days</h3>
      {{#each exercise}}
      {{#if @first}}
      <input type="hidden" name='username' value="{{User.username}}" /
      {{/if}}

      <li>
        <p>Date: {{date}}</p>
        <p>Height: {{height}}</p>
        <p>Weight: {{weight}}</p>
        <p>Push-Ups: {{pushups}}</p>
        <p>Sit-Ups: {{situps}}</p>
        <p>Pull-Ups: {{pullups}}</p>
        <p>Run Time: {{runtime}}</p>
      </li>
      {{/each}}

    </div>
  </div>
    <div id="graphicalView">
    <h3>Graphical View</h3>
    <button id="emailButton" type="button" >Email Results</button>
    </div>
  </div>
  <script src="/js/email.js"></script>

